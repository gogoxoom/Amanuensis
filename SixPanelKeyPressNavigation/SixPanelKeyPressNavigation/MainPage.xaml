<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:MetroLibrary="clr-namespace:MetroLibrary;assembly=MetroLibrary" xmlns:local="clr-namespace:SixPanelKeyPressNavigation"
	mc:Ignorable="d"
	x:Class="SixPanelKeyPressNavigation.MainPage">

	<Grid x:Name="LayoutRoot" HorizontalAlignment="Center" VerticalAlignment="Center">
		<MetroLibrary:TabletDevice d:LayoutOverrides="Width, Height" HorizontalAlignment="Left" VerticalAlignment="Top" Background="#FF6868F9">
			<MetroLibrary:TabletDevice.DeviceContent>
				<local:SixPanelPage/>
			</MetroLibrary:TabletDevice.DeviceContent>
		</MetroLibrary:TabletDevice>
	</Grid>
</UserControl>